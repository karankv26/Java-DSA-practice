
import java.util.*;

public class karan {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the number: ");
        long n = sc.nextInt();
        long ans = 0;
        while (n != 0) {
            n = n / 10;
            ans++;
        }
        System.out.println(ans);

    }
}
